{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/ui/src/env.d.ts",
    "src/ui/src/**/*",
    "src/ui/src/**/*.vue",
    "src/preload/*.d.ts"
  ],
  "compilerOptions": {
    "moduleResolution": "bundler",
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@ui/*": [
        "src/ui/src/*"
      ]
    }
  }
}
